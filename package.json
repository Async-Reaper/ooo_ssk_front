{
  "name": "react-ssk",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 5173",
    "build": "tsc && vite build",
    "lint:ts": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "lint:ts:fix": "eslint \"**/*.{ts,tsx}\" --fix",
    "lint:scss": "npx stylelint \"**/*.scss\"",
    "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "icons:optimise": "yarn svgo -f src/shared/libs/icons/__sources --recursive",
    "icons:transpile": "yarn svgr src/shared/libs/icons/__sources -d ./src/shared/libs/icons/__generated --config-file src/shared/libs/icons/__svgr/configs/svgr.universal.cjs --typescript",
    "icons": "yarn icons:optimise && yarn icons:transpile",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "framer-motion": "^10.16.4",
    "konva": "^9.3.20",
    "node": "^20.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.10",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "sass": "^1.66.1",
    "url": "^0.11.1"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-redux": "^7.1.26",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard-scss": "^10.0.0",
    "svgo": "^2.7.0",
    "typescript": "*",
    "vite": "^4.4.5",
    "vite-plugin-svgr": "^4.1.0",
    "yarn": "^1.22.21"
  }
}
